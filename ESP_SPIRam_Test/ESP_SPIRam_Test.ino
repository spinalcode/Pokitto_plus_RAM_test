 ***************************************************************************/
#include <SPI.h>

#include <ESP8266Spiram.h>
ESP8266Spiram Spi_ram;

#define MISO_pin 12
#define MOSI_pin 13
#define SCLK_pin 14
#define CS_pin 5

void release_control() {
  // Surrender the SPI pins
  pinMode(MISO_pin, INPUT); // MISO
  pinMode(MOSI_pin, INPUT); // MOSI
  pinMode(SCLK_pin, INPUT); // SCLK
  pinMode(CS_pin, INPUT); // CS
  // Turn the LED off
  digitalWrite(LED_BUILTIN, HIGH);  // Turn the LED off by making the voltage HIGH
}

void setup() {
  pinMode(LED_BUILTIN, OUTPUT);     // Initialize the LED_BUILTIN pin as an output
  Serial.begin(115200);
  Serial.println("Starting");

  Spi_ram.begin();
  Spi_ram.setSeqMode();
  release_control();
}


void sendToRAM(int location, uint8_t* data, int howLong) {
  while (digitalRead(CS_pin) == 0) {
    yield();
  }
  SPI.begin();
  pinMode(CS_pin, OUTPUT);
  digitalWrite(CS_pin, LOW);
  // Turn on the LED (not required)
  digitalWrite(LED_BUILTIN, LOW);

  Spi_ram.write(location, data, howLong);

  release_control();
}

void getFromRAM(int location, uint8_t* data, int howLong) {
  while (digitalRead(CS_pin) == 0) {
    yield();
  }
  SPI.begin();
  pinMode(CS_pin, OUTPUT);
  digitalWrite(CS_pin, LOW);
  // Turn on the LED (not required)
  digitalWrite(LED_BUILTIN, LOW);

  Spi_ram.read(location, data, howLong);

  release_control();
}


#define AMOUNT 1536
void loop() {
  uint8_t wRam[AMOUNT];

  getFromRAM(0x00, wRam, AMOUNT);

  wRam[AMOUNT]=0;
  char tempText[AMOUNT];
  sprintf(tempText,"%s",wRam);
  Serial.println(tempText);

  sendToRAM(0x00, rawData, AMOUNT);

  delay(1000);

}
